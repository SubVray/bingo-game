{
	"printWidth": 80,
	"semi": false,
	"singleQuote": false,
	"jsxSingleQuote": false,
	"quoteProps": "consistent",
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"endOfLine": "lf",
	"arrowParens": "always",
	"singleAttributePerLine": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svg",
			"options": {
				"parser": "html"
			}
		}
	]
}
